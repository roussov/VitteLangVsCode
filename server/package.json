{
  "name": "vitte-vscode-server",
  "private": true,
  "version": "0.1.0",
  "type": "commonjs",
  "main": "out/server.js",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "tsc -b",
    "watch": "tsc -b -w",
    "clean": "tsc -b --clean",
    "start": "node --enable-source-maps out/server.js",
    "prepare": "npm run build",
    "lint": "tsc --noEmit"
  },
  "dependencies": {
    "vscode-languageserver": "^9.0.1",
    "vscode-languageserver-textdocument": "^1.0.8"
  },
  "devDependencies": {
    "typescript": "^5.0.0",
    "@types/node": "^20.0.0"
  }
}
